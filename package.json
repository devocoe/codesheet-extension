{
  "name": "codesheet",
  "displayName": "Codesheet",
  "publisher": "Piyush Kumar",
  "description": "a simple code cheatsheet extension",
  "repository": {
    "url": "https://github.com/piyush816/codesheet-extension.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codesheet.sheets"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codesheet.sheets",
        "title": "Codesheets"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
